<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.bestcell">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AndroidPhoneActivity"
            android:label="Select Android Brand" />
        <activity
            android:name=".IphonePhoneActivity"
            android:label="Select Iphone" />
        <activity
            android:name=".WindowsPhoneActivity"
            android:label="Select Windows Phone" />
        <activity android:name=".DetailsActivity"></activity>
    </application>

</manifest>